AA,aAAA,EAAA;AAJA,OADA,EAOA;AACA,QAAA,GAAA,EAAA,KADA;AAEA,QAAA,KAAA,EAAA,MAFA;AAGA,QAAA,QAAA,EAAA,IAHA;AAIA,QAAA,KAAA,EAAA;AAJA,OAPA,EAaA;AACA,QAAA,GAAA,EAAA,OADA;AAEA,QAAA,KAAA,EAAA,QAFA;AAGA,QAAA,QAAA,EAAA,IAHA;AAIA,QAAA,KAAA,EAAA;AAJA,OAbA,EAmBA;AACA,QAAA,GAAA,EAAA,OADA;AAEA,QAAA,KAAA,EAAA,QAFA;AAGA,QAAA,QAAA,EAAA,IAHA;AAIA,QAAA,KAAA,EAAA;AAJA,OAnBA,EAyBA;AACA,QAAA,GAAA,EAAA,QADA;AAEA,QAAA,KAAA,EAAA,QAFA;AAGA,QAAA,QAAA,EAAA,IAHA;AAIA,QAAA,KAAA,EAAA;AAJA,OAzBA,EA+BA;AAAA,QAAA,GAAA,EAAA,SAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OA/BA,CAhBA;AAiDA,MAAA,SAAA,EAAA,CAjDA;AAkDA,MAAA,WAAA,EAAA,CAlDA;AAmDA,MAAA,OAAA,EAAA,EAnDA;AAoDA,MAAA,WAAA,EAAA,CACA,CADA,EAEA,EAFA,EAGA,EAHA,EAIA,EAJA,EAKA,GALA,EAMA;AAAA,QAAA,KAAA,EAAA,MAAA,CAAA,gBAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OANA,CApDA;AA4DA,MAAA,MAAA,EAAA,EA5DA;AA6DA,MAAA,QAAA,EAAA,KA7DA;AA8DA,MAAA,aAAA,EAAA,KA9DA;AA+DA,MAAA,MAAA,EAAA,IA/DA;AAgEA,MAAA,QAAA,EAAA,CAAA,MAAA,EAAA,IAAA,EAAA,OAAA,EAAA,OAAA;AAhEA,KAAA;AAkEA,GApEA;AAqEA,EAAA,OArEA,qBAqEA;AAAA;;AACA,IAAA,KAAA,CAAA,8CAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA;AAAA,aAAA,GAAA,CAAA,IAAA,EAAA;AAAA,KADA,EAEA,IAFA,CAEA,UAAA,WAAA,EAAA;AACA,UAAA,MAAA,GAAA,EAAA;AACA,UAAA,KAAA,GAAA,WAAA,CAAA,KAAA;AACA,MAAA,WAAA,CAAA,OAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,GAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA;AAAA,iBAAA,GAAA,CAAA,IAAA,EAAA;AAAA,SADA,EAEA,IAFA,CAEA,UAAA,WAAA,EAAA;AACA,cAAA,KAAA,GAAA,WAAA,CAAA,KAAA;AACA,UAAA,WAAA,CAAA,KAAA,GAAA,KAAA,CAAA,CAAA,CAAA,CAAA,IAAA,CAAA,IAAA;AACA,UAAA,WAAA,CAAA,KAAA,GACA,KAAA,CAAA,CAAA,CAAA,IAAA,IAAA,GAAA,KAAA,CAAA,CAAA,CAAA,CAAA,IAAA,CAAA,IAAA,GAAA,SADA;AAEA,UAAA,WAAA,CAAA,MAAA,GAAA,WAAA,CAAA,OAAA,CAAA,aAAA;AACA,cAAA,IAAA,GAAA,OAAA,CAAA,IAAA,CAAA,OAAA,CAAA,GAAA,EAAA,GAAA,CAAA;AACA,UAAA,WAAA,CAAA,GAAA,GAAA,IAAA,CAAA,MAAA,CAAA,CAAA,EAAA,WAAA,KAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,WAAA;AACA,SAXA,EAYA,IAZA,CAYA,YAAA;AACA,cAAA,KAAA,IAAA,MAAA,CAAA,MAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,KAAA,CAAA,QAAA,EAAA,MAAA;AACA,YAAA,KAAA,CAAA,SAAA,GAAA,QAAA,CAAA,MAAA;AACA;AACA,SAjBA;AAkBA,OAnBA;AAoBA,KAzBA,EADA,CA2BA;AACA,GAjGA;AAkGA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,aADA,EACA;AACA;AACA,WAAA,SAAA,GAAA,aAAA,CAAA,MAAA;AACA,WAAA,WAAA,GAAA,CAAA;AACA;AALA;AAlGA,CAAA","sourcesContent":["<template>\n  <b-container fluid>\n    <!-- User Interface controls -->\n    <b-row>\n      <b-form-input\n        v-model=\"filter\"\n        type=\"search\"\n        placeholder=\"Type to filter data\"\n      ></b-form-input>\n      <b-col sm=\"5\" md=\"6\" class=\"my-1\">\n        <b-form-group\n          label=\"Elements per page\"\n          label-for=\"per-page-select\"\n          label-cols-sm=\"6\"\n          label-cols-md=\"4\"\n          label-cols-lg=\"3\"\n          label-align-sm=\"right\"\n          label-size=\"sm\"\n          class=\"mb-0\"\n        >\n          <b-form-select\n            id=\"per-page-select\"\n            v-model=\"perPage\"\n            :options=\"pageOptions\"\n            size=\"sm\"\n          ></b-form-select>\n        </b-form-group>\n      </b-col>\n    </b-row>\n\n    <!-- Main table element -->\n    <b-table\n      :items=\"items\"\n      :fields=\"fields\"\n      :current-page=\"currentPage\"\n      :per-page=\"perPage\"\n      :filter=\"filter\"\n      :filter-included-fields=\"filterOn\"\n      :sort-by.sync=\"sortBy\"\n      :sort-desc.sync=\"sortDesc\"\n      :sort-direction=\"sortDirection\"\n      stacked=\"md\"\n      show-empty\n      small\n      striped\n      responsive\n      @filtered=\"onFiltered\"\n    >\n      <template #cell(name)=\"row\">\n        {{ row.value.first }} {{ row.value.last }}\n      </template>\n\n      <template #cell(sprite)=\"data\">\n        <img :src=\"data.item.sprite\" />\n      </template>\n\n      <template #cell(actions)=\"row\">\n        <b-button size=\"sm\" @click=\"row.toggleDetails\">\n          {{ row.detailsShowing ? \"Hide\" : \"Show\" }} Details\n        </b-button>\n      </template>\n\n      <template #row-details=\"row\">\n        <b-card>\n          <h1>{{ row.item.nem }}</h1>\n          <div>\n            <h2>Habilidades</h2>\n            <ul>\n              <li v-for=\"ability in row.item.abilities\" :key=\"ability.name\">\n                {{ ability.ability.name }}\n                {{ ability.is_hidden ? \"si\" : \"no\" }} es oculta.\n              </li>\n            </ul>\n          </div>\n          <div>\n            <h2>Movimientos</h2>\n            <b-table\n              striped\n              responsive\n              :items=\"row.item.moves\"\n              :fields=\"moveFields\"\n            ></b-table>\n          </div>\n          <h2>Sprites</h2>\n          <div\n            v-for=\"(gen, index) in row.item.sprites.versions\"\n            :key=\"gen.name\"\n          >\n            <h3>{{ index }}</h3>\n            <div v-for=\"(game, index2) in gen\" :key=\"game.id\">\n              <div>\n                <h4>{{ index2 }}</h4>\n                <ul style=\"display: flex; justify-content: space-evenly;\">\n                  <!-- eslint-disable vue/no-use-v-if-with-v-for,vue/no-confusing-v-for-v-if -->\n                  <li v-for=\"(sprite, index3) in game\" v-if=\"sprite!=null && index3!='animated'\" :key=\"index3\">\n                    <div>\n                      <h5>{{ index3 }}</h5>\n                      <img :src=\"sprite\">\n                    </div>\n                  </li>\n                </ul>\n              </div>\n            </div>\n          </div>\n        </b-card>\n      </template>\n    </b-table>\n\n    <b-pagination\n      v-model=\"currentPage\"\n      :total-rows=\"totalRows\"\n      :per-page=\"perPage\"\n    ></b-pagination>\n  </b-container>\n</template>\n\n<script>\nvar pokemons = [];\nexport default {\n  data() {\n    return {\n      items: pokemons,\n      moveFields: [\n        {\n          key: \"move.name\",\n          label: \"Nombre\",\n          sortable: true,\n          sortDirection: \"desc\",\n        },\n        {\n          key: \"version_group_details[0].level_learned_at\",\n          label: \"Nivel\",\n          sortable: true,\n          sortDirection: \"desc\",\n        },\n      ],\n      fields: [\n        {\n          key: \"id\",\n          label: \"Id\",\n          sortable: true,\n          sortDirection: \"desc\",\n        },\n        {\n          key: \"nem\",\n          label: \"Name\",\n          sortable: true,\n          class: \"text-center\",\n        },\n        {\n          key: \"type1\",\n          label: \"Type 1\",\n          sortable: true,\n          class: \"text-center\",\n        },\n        {\n          key: \"type2\",\n          label: \"Type 2\",\n          sortable: true,\n          class: \"text-center\",\n        },\n        {\n          key: \"sprite\",\n          label: \"Sprite\",\n          sortable: true,\n          class: \"image\",\n        },\n        { key: \"actions\", label: \"Actions\" },\n      ],\n      totalRows: 1,\n      currentPage: 1,\n      perPage: 10,\n      pageOptions: [\n        5,\n        10,\n        25,\n        50,\n        100,\n        { value: Number.MAX_SAFE_INTEGER, text: \"show all\" },\n      ],\n      sortBy: \"\",\n      sortDesc: false,\n      sortDirection: \"asc\",\n      filter: null,\n      filterOn: [\"name\", \"id\", \"type1\", \"type2\"],\n    };\n  },\n  mounted() {\n    fetch(\"https://pokeapi.co/api/v2/pokemon?limit=1200\")\n      .then((res) => res.json())\n      .then((finalResult) => {\n        var pokems = [];\n        var total = finalResult.count;\n        finalResult.results.forEach((pokemon) => {\n          fetch(pokemon.url)\n            .then((res) => res.json())\n            .then((finalResult) => {\n              var types = finalResult.types;\n              finalResult.type1 = types[0].type.name;\n              finalResult.type2 =\n                types[1] != null ? types[1].type.name : undefined;\n              finalResult.sprite = finalResult.sprites.front_default;\n              var name = pokemon.name.replace(\"-\", \" \");\n              finalResult.nem = name.charAt(0).toUpperCase() + name.slice(1);\n              pokems.push(finalResult);\n            })\n            .then(() => {\n              if (total == pokems.length) {\n                pokemons.push.apply(pokemons, pokems);\n                this.totalRows = pokemons.length;\n              }\n            });\n        });\n      });\n    // Set the initial number of items\n  },\n  methods: {\n    onFiltered(filteredItems) {\n      // Trigger pagination to update the number of buttons/pages due to filtering\n      this.totalRows = filteredItems.length;\n      this.currentPage = 1;\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.pagination {\n  display: flex;\n  justify-content: center;\n}\n</style>\n"],"sourceRoot":"src/components"}]}